// portofolio-container-adapter-queue.cpp : This file contains the 'main' function. Program execution begins and ends there.
#include <iostream>
#include <queue>
#include <algorithm>
#include <string>

template<typename T>
void display(std::queue<T> q) {
    std::cout << "[ ";
    while (!q.empty()) {
        T elem = q.front();
        q.pop();
        std::cout << elem << " ";
    }
    std::cout << "]" << std::endl;
}

void display(std::queue<int> q) {
    std::cout << "[ ";
    while (!q.empty()) {
        auto elem = q.front();
        q.pop();
        std::cout << elem << " ";
    }
    std::cout << "]" << std::endl;
}

void display(std::queue<std::string> s) {
    std::cout << "[";
    while (!s.empty()) {
        auto elem = s.front();
        s.pop();
        std::cout << elem << " ";
    }
    std::cout << "]" << std::endl;
}

int main()
{
    std::queue<int> q;
    for (int i : {1, 2, 3, 4, 5})
        q.push(i);
    display(q);
    std::queue<std::string> q1;
    q1.push("Jenny");
    q1.push("Elizabeth");
    q1.push("Gabriel");
    display(q1);
      std::cout << "Front: " << q.front() << std::endl;
    std::cout << "Back: " << q.back() << std::endl;
    std::cout << "Front: " << q1.front() << std::endl;
    std::cout << "Back: " << q1.back() << std::endl;
    q.push(101);
    display(q);
    q.pop();
    q.pop();
    display(q);
    q1.push("Jennifer");
    display(q1);
    q1.pop();
    q1.pop();
    display(q1);
    std::cout << "Front: " << q.front() << std::endl;
    std::cout << "Back: " << q.back() << std::endl;
    std::cout << "Front: " << q1.front() << std::endl;
    std::cout << "Back: " << q1.back() << std::endl;
      while (!q.empty())
        q.pop();
    display(q);
    std::cout << "Size: " << q.size() << std::endl;
    while (!q1.empty())
        q1.pop();
    display(q1);
    std::cout << "Size: " << q1.size() << std::endl;
       q.push(10);
    q.push(100);
    q.push(1000);
    q.push(10001);
    display(q);
    std::cout << "Size: " << q.size() << std::endl;
    q1.push("C++");
    q1.push("Coding");
    q1.push("is");
    q1.push("awesome.");
    display(q1);
    std::cout << "Size: " << q1.size() << std::endl;
    std::cout << "Front: " << q.front() << std::endl;
    std::cout << "Back: " << q.back() << std::endl;
    std::cout << "Front: " << q1.front() << std::endl;
    std::cout << "Back: " << q1.back() << std::endl;
    return 0;
}
